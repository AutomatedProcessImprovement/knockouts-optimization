{
  "$schema": "../../config_schema.json",
  "log_path": "inputs/synthetic_example/Synthetic Example (KO Order IO 2).xes",
  "output": "./outputs",
  "redesign_results_file_path": "outputs/ko_reorder_io_1_timest.txt",
  "cache_dir": "cache/synthetic_example",
  "start_activity": "Credit application received",
  "post_knockout_activities": [
    "Notify rejection"
  ],
  "rule_discovery_algorithm": "RIPPER",
  "confidence_threshold": 0.5,
  "support_threshold": 0.1,
  "max_rules": 3,
  "max_rule_conds": 1,
  "k": 2,
  "n_discretize_bins": 10,
  "dl_allowance": 1,
  "prune_size": 0.8,
  "always_force_recompute": true,
  "print_rule_discovery_stats": true,
  "grid_search": false,
  "custom_log_preprocessing_function": "enrich_log_for_synthetic_example_validation",
  "read_options": {
    "one_timestamp": true
  }
}